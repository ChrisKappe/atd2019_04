{
    "source": {
        "watch": [
            "./src/**/*.js",
            "./src/**/*.jsx",
            "./src/**/*.css"
        ],
        "watchTimeout": 1000,
        "bundle": "./dist/bundle.js"
    },
    "controlAddInScripts": {
        "watch": [
            "./public/.controlAddIn/*.js"
        ],
        "scripts": "./public/.controlAddIn/*.js",
        "watchTimeout": 1000
    },
    "target": {
        "scripts": "../AL/src/add-in/scripts"
    }
}
